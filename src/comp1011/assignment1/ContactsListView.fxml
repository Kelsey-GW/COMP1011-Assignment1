<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="675.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ContactsListViewController">
   <children>
      <Label fx:id="yourContactsLabel" layoutX="272.0" layoutY="28.0" text="Your Contacts">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="contactsTable" layoutX="22.0" layoutY="82.0" prefHeight="200.0" prefWidth="629.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="58.0" text="ID" />
          <TableColumn fx:id="firstNameColumn" prefWidth="92.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="119.0" text="Last Name" />
            <TableColumn fx:id="addressColumn" prefWidth="214.0" text="Address" />
            <TableColumn fx:id="phoneNumberColumn" prefWidth="145.0" text="Phone Number" />
        </columns>
      </TableView>
      <Button fx:id="makeNewContact" layoutX="566.0" layoutY="315.0" mnemonicParsing="false" onAction="#newContactButtonPushed" text="New Contact" />
   </children>
</AnchorPane>
